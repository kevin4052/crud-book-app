{{#if user}}
  <h2>Welcome, {{ user.username }}!</h2>
  <p>This is your profile page my friend!</p>
  <button>
    <a href="/user/user-profile">Reload the page</a>
  </button>

  <div>
    <h3>wish list</h3>
    <ul>
      {{#each user.books}}
      <li>{{ this.title }}</li>
    {{/each}}
    </ul>
    
  </div>

{{else}}
  <p>Still no logged in user, sorry!</p>
{{/if}}


