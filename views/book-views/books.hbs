<h1>Your books</h1>
<a href="/books/add-book"><button>Add a book</button></a>
<div class="container">
      {{#each books}}
        <div class="infoCard">
            <h3>{{ this.title }}</h3>
            <a href="/books/details/{{ this._id }}"><img src="{{ this.coverArt }}" alt="{{ this.title }} cover art"></a>            
        </div>
    {{/each}}  
</div>